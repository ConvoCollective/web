{
  "ava": {
    "failWithoutAssertions": true,
    "files": [
      "src/test/**/*.test.js"
    ]
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@tailwindcss/typography": "^0.3.1",
    "@types/node": "^16.11.6",
    "@typesafejs/eslint-config": "^0.2.5",
    "alpinejs": "^2.6.0",
    "ava": "^3.15.0",
    "browser-sync": "^2.27.6",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "html-metadata": "^1.7.1",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.21",
    "luxon": "^1.25.0",
    "markdown-it": "^12.3.2",
    "markdown-it-for-inline": "^0.1.1",
    "markdown-spellcheck": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "prismjs": "^1.21.0",
    "tailwindcss": "^2.2.17",
    "time-require": "^0.1.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "autoprefixer": "^10.3.7",
    "core-js": "3",
    "html-react-parser": "^1.4.10",
    "lodash.filter": "^4.6.0",
    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-link-preview": "^1.1.1",
    "postcss": "^8.3.11",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "yarn": "^1.22.18"
  },
  "scripts": {
    "browsersync": "browser-sync start --server '_site' --files '_site' --port 8080 --no-notify --no-open",
    "build": "run-s build:css build:debug",
    "build:debug": "(set DEBUG=* & npx slinkity) || true",
    "build:site": "cross-env NODE_ENV=production eleventy",
    "build:css": "postcss site/static/css/tailwind.css --o _site/static/css/style.css",
    "build:css:slinkity": "postcss site/static/css/tailwind.css --o _styles/styles.css",
    "compile": "tsc",
    "cms": "npx netlify-cms-proxy-server",
    "dev:site": "eleventy --serve --incremental",
    "dev:css": "postcss site/static/css/tailwind.css --o _site/static/css/style.css --watch",
    "dev:css:slinkity": "postcss site/static/css/tailwind.css --o _styles/styles.css --watch",
    "lint": "eslint src/**/*.js",
    "spellcheck": "mdspell --en-us -n -r site/**/*.md site/**/**/*.md",
    "start": "npm-run-all --parallel dev:css dev:site browsersync",
    "test": "ava"
  },
  "version": "0.3.0"
}
